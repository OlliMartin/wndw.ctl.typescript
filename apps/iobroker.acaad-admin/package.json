{
  "name": "@ollimartin/iobroker.acaad-admin",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^19.0.0",
    "react-app-polyfill": "^3.0.0",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^19.0.0",
    "react-refresh": "^0.11.0",
    "@ollimartin/react-scripts": "workspace:*",
    "web-vitals": "~4.2.4",
    "typescript": "~5.7.2",
    "ts-loader": "~9.5.1",
    "@types/react": "~19.0.2",
    "@types/react-dom": "~19.0.2",
    "@ollimartin/eslint-config": "workspace:*"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "react-scripts lint src",
    "lint-staged": "react-scripts lint-staged"
